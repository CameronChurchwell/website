<script defer src="{{url_for('static', filename='js/login.js') }}"></script>
<script defer src="{{url_for('static', filename='build/main.js') }}"></script>

<div id="main-content">
    <div id="form-container" class="main-content-callout">
        <form id="login-form">
            <div class="label-input-container">
                <label for="email">Email:</label>
                <input type="email", id="email"></input><br><br>
            </div>
            <div class="label-input-container">
                <label for="password">Password:</label>
                <input type="password", id="password"><br><br>
            </div>
            <button class="submit button" id="submit-credentials">Submit!</button>
        </form>
    </div>
</div>

{% endblock %}