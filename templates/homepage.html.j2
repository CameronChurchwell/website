                            <path d="" id="shape-wireframe" stroke="#ff7700" stroke-opacity="1" stroke-width="10" stroke-linecap="round" stroke-linejoin="round" fill="none"/> #}

                            {# <path id="original" d="" stroke="#b200e3" stroke-width="10" stroke-linecap="round" stroke-linejoin="round" fill="#ffffff" shape-rendering="optimizeSpeed"/>
                            <use href="#original" x="100"/>
                            <use href="#original" x="-100"/>
                            <use href="#original" x="-150"/>
                            <use href="#original" x="150"/> #}
                            
                            <path d="" stroke="#b200e3" stroke-width="10" stroke-linecap="round" stroke-linejoin="round" fill="#ffffff" shape-rendering="optimizeSpeed"/>
